            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ylbx7">

            </ImageView>
            <TextView
                android:layout_width="200px"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:gravity="center_horizontal"
                android:text="机关养老个人缴费明细查询">

            </TextView>
        </LinearLayout>

        <LinearLayout

            android:id="@+id/lin_yl_08"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ylbx8">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:gravity="center_horizontal"
                android:text="机关事业单位退休人员领取基本养老金查询">

            </TextView>
        </LinearLayout>

        <LinearLayout

            android:id="@+id/lin_yl_09"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ylbx9">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:gravity="center_horizontal"
                android:text="机关事业单位社会保险个人缴费信息查询">

            </TextView>
        </LinearLayout>
        <LinearLayout

            android:id="@+id/lin_yl_10"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ylbx10">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:gravity="center_horizontal"
                android:text="转移信息综合查询">

            </TextView>
        </LinearLayout>



    </LinearLayout>
</LinearLayout>