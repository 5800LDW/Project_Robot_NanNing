<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="1500px"
    android:layout_height="wrap_content"
    android:id="@+id/lin_ylbx"
    android:background="#fff">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/lin_yiliao_01"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ybxxcx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="医保信息查询">

            </TextView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_yiliao_02"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ylxfcx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="医保待遇支付信息查询">

            </TextView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_yiliao_03"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_mzmxbcx2">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="门诊明慢性病查询">

            </TextView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_yiliao_04"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ydylfycx">

            </ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20px"
                android:text="异地医疗费用查询"
                android:textColor="@color/c_black_1"
                android:textSize="15sp">

            </TextView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lin_yiliao_05"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_sydycx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="生育待遇查询">

            </TextView>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin_yiliao_06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50px"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ydjybacx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="异地就医备案查询">

            </TextView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lin_yiliao_07"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_yljfmxcx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="医疗缴费明细查询">

            </TextView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_yiliao_08"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ylzhszmx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="医疗账户收支明细查询">

            </TextView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_yiliao_09"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_syjfmxcx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="生育缴费明细查询">

            </TextView>
        </LinearLayout>



    </LinearLayout>
</LinearLayout>