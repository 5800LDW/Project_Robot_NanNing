<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="1500px"
    android:layout_height="wrap_content"
    android:id="@+id/lin_gsbx"
    android:background="#fff">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/lin_sbk_01"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ybxxcx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="社保卡密码重置">

            </TextView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_sbk_02"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ylxfcx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="社保卡激活">

            </TextView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_sbk_03"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_mzmmxbcx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="社保卡挂失解挂">

            </TextView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_sbk_04"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ydylfycx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="社保卡进度查询">

            </TextView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lin_sbk_05"
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:layout_marginLeft="50px"
            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_sydycx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="社保卡密码修改">

            </TextView>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin_sbk_06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50px"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="260px"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:orientation="vertical">

            <ImageView
                android:layout_width="130px"
                android:layout_height="130px"
                android:background="@mipmap/logo_ydjybacx">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/c_black_1"
                android:textSize="15sp"
                android:layout_marginTop="20px"
                android:text="社保卡状态查询">

            </TextView>
        </LinearLayout>





    </LinearLayout>


</LinearLayout>