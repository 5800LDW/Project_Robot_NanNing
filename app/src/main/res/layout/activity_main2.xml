<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/logo_title"
        android:layout_marginLeft="@dimen/dimen_30">
    </TextView>

    <ImageView
        android:id="@+id/img_bg"
        android:layout_below="@+id/tv_title"
        android:layout_width="650dp"
        android:layout_height="@dimen/dimen_441"
        android:background="@mipmap/logo_nanning"
        android:layout_marginTop="@dimen/dimen_30"
        android:layout_marginLeft="@dimen/dimen_100"></ImageView>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/dimen_90"
        android:paddingRight="@dimen/dimen_30"
        android:layout_alignParentRight="true">
        <Button
            android:id="@+id/btn_sbkxxcx"
            android:layout_width="269dp"
            android:layout_height="80dp"
            android:background="@mipmap/btn_sbxxcx">
        </Button>
        <Button
            android:id="@+id/btn_ldjy"
            style="@style/style_button_logo2"
            android:background="@mipmap/btn_ldjyfw">
        </Button>
        <Button
            android:id="@+id/btn_sbyw"
            style="@style/style_button_logo2"
            android:background="@mipmap/btn_sbywjb">
        </Button>
        <Button
            android:id="@+id/btn_sscard_manager"
            style="@style/style_button_logo2"
            android:background="@mipmap/btn_sbkgl">
        </Button>
        <Button
            android:id="@+id/btn_qtyw"
            style="@style/style_button_logo2"
            android:background="@mipmap/btn_qtywjb">
        </Button>

    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/img_bg">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/dimen_10"
            android:background="@mipmap/logo_shouyedise"
            android:layout_alignParentBottom="true"
            android:gravity="center_vertical"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/lin_grjb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginLeft="@dimen/dimen_100"
                android:orientation="vertical">
                <ImageView
                    style="@style/style_button_logo"
                    android:background="@mipmap/btn_jbxxcx"
                    ></ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/c_black_1"
                    android:layout_marginTop="@dimen/dimen_10"
                    android:textSize="16sp"
                    android:text="个人基本信息查询"></TextView>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/lin_ylzh"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"

                android:layout_marginLeft="@dimen/dimen_40"
                android:orientation="vertical">
                <ImageView
                    style="@style/style_button_logo"
                    android:background="@mipmap/btn_ylszmx"
                    ></ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/c_black_1"
                    android:layout_marginTop="@dimen/dimen_10"
                    android:textSize="16sp"
                    android:text="医疗账户收支明细打印"></TextView>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/lin_ywbl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"

                android:layout_marginLeft="@dimen/dimen_40"
                android:orientation="vertical">
                <ImageView
                    style="@style/style_button_logo"
                    android:background="@mipmap/btn_ywcldyjg"
                    ></ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/c_black_1"
                    android:layout_marginTop="@dimen/dimen_10"
                    android:textSize="16sp"
                    android:text="业务办理结果打印"></TextView>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/lin_cbjf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"

                android:layout_marginLeft="@dimen/dimen_40"
                android:orientation="vertical">
                <ImageView
                    style="@style/style_button_logo"
                    android:background="@mipmap/btn_cbjfzmdy"
                    ></ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/c_black_1"
                    android:layout_marginTop="@dimen/dimen_10"
                    android:textSize="16sp"
                    android:text="参保缴费证明打印"></TextView>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_135"
            android:gravity="center"
            android:layout_alignParentRight="true"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:paddingBottom="@dimen/dimen_50"
            android:layout_marginRight="@dimen/dimen_150"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/lin_back"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_50"
                android:background="@mipmap/btn_fanhui"
                android:gravity="center">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/logo_people"></ImageView>
                <TextView
                    android:id="@+id/tv_back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="返回"
                    android:layout_marginLeft="@dimen/dimen_10"
                    android:textColor="#fff"
                    android:textSize="20sp">

                </TextView>

            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="@dimen/dimen_135"
            android:gravity="center"
            android:layout_alignParentRight="true"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:paddingBottom="@dimen/dimen_50"
            android:layout_marginRight="@dimen/dimen_20"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/lin_login"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_50"
                android:background="@mipmap/btn_login"
                android:gravity="center">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/logo_people"></ImageView>
                <TextView
                    android:id="@+id/tv_login"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/app_text_login"
                    android:layout_marginLeft="@dimen/dimen_10"
                    android:textColor="#fff"
                    android:textSize="20sp">

                </TextView>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_10"
                    android:background="@mipmap/logo_right"></ImageView>

            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="@dimen/dimen_50"
            android:paddingBottom="@dimen/dimen_20"
            android:layout_marginTop="@dimen/dimen_10"
            android:textColor="@color/c_black_1"
            android:text="1213123123">

        </TextView>


    </RelativeLayout>

</RelativeLayout>